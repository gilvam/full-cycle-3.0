{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
	  "parser": {
	    "syntax": "typescript",
	    "jsx": false,
	    "dynamicImport": true,
	    "privateMethod": false,
	    "functionBind": false,
	    "exportDefaultFrom": false,
	    "exportNamespaceFrom": false,
	    "decorators": true,
	    "decoratorsBeforeExport": false,
	    "topLevelAwait": false,
	    "importMeta": false
	  },
	  "transform": null,
	  "target": "es2016",
	  "loose": false,
	  "externalHelpers": false,
	  "keepClassNames": false // Requires v1.2.50 or upper and requires target to be es2016 or upper.
	  //,
		// "baseUrl": "./",
		// "paths": {
		//    "@domain/*": ["src/domain/*"]
		//  }
	},
	"minify": false
}
